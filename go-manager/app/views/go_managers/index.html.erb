<p id="notice"><%= notice %></p>

<h1>Go Managers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @go_managers.each do |go_manager| %>
      <tr>
        <td><%= link_to 'Show', go_manager %></td>
        <td><%= link_to 'Edit', edit_go_manager_path(go_manager) %></td>
        <td><%= link_to 'Destroy', go_manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Go Manager', new_go_manager_path %>
